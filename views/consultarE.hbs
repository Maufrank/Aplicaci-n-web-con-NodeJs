<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet">
    <title>Equipos</title>
</head>
<body>
    <main class="container">
        {{> navegacion}}
        <table class="table table-border">
            <tr>
                <th>Nombre Equipo</th>
                <th>Fecha Creacion</th>
                <th>Titulos</th>
                <th>Liga</th>
                <th>Division</th>
                <th>Nacionalidad</th>
                <th>Editar</th>
                <th>Eliminar</th>
            </tr>
{{#each data}}
            <tr>
                <td>{{nombreE}}</td>
                <td>{{fechaE}}</td>
                <td>{{titulos}}</td>
                <td>{{liga}}</td>
                <td>{{division}}</td>
                <td>{{nacionalidad}}</td>
                <td><a href="/eliminarEquipo/{{_id}}">Eliminar</a></td>
                <td><a href="/actualizarEquipo/{{_id}}">Editar</a></td>
            </tr>
{{/each}} 
        </table>
    </main>
</body>
</html>